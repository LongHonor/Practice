#include<stdio.h>

void input(int *x,int *y, int *z);
void output(int *x, int *y, int *z);
int main(){
	
	int m,n,o;
	int *p=&m,*q=&n,*r=&o;

	input(p,q,r);
	output(p,q,r);

	return 0;
}

void input(int *x,int*y,int *z){
	scanf("%d%d%d",x,y,z);
}

void output(int *x,int *y, int *z){
	printf("%d %d %d\n",*x,*y,*z);
} 
