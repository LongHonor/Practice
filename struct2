#include <stdio.h>

typedef struct TIME{
	int h;
	int m;
	int s;
}t; 

int main(){
	t t1 = {0,0,0};
	t t2 = t1, t3 = t1;
	int sflag = 0,mflag = 0;
	
	scanf("%d%d%d",&t1.h,&t1.m,&t1.s);
	scanf("%d%d%d",&t2.h,&t2.m,&t2.s);
	
	if(t2.s - t1.s > 0){
		t3.s = t2.s - t1.s;
	}
	else if(t2.s - t1.s < 0){
		t3.s = 60-t1.s +t2.s;
		sflag = 1;
	}
	else{
		t3.s = 0;
	}
	
	if(sflag == 1){
		if(t2.m - t1.m > 0){
			t3.m = t2.m-1-t1.m;
		}
		else if(t2.m - t1.m <0){
			t3.m = 60- t1.m + t2.m -1;
			mflag = 1;
		}
		else{
			t3.m = 59;
			mflag = 1;
		}
	}
	else{
		if(t2.m - t1.m > 0){
			t3.m = t2.m - t1.m;
		}
		else if(t2.m - t1.m < 0){
			t3.m = 60 - t1.m +t2.m;
			mflag = 1;
		}
		else{
			t3.m = 0;
		}
	}
	if(mflag == 1){
		if(t2.h-t1.h>0){
			t3.h = t2.h -t1.h -1;
		}
		else t3.h = 0;
	}
	else{
		if(t2.h-t1.h>0){
			t3.h= t2.h - t1.h;
		}
		else t3.h = 0;
	}
	
	printf("%d %d %d\n",t3.h,t3.m,t3.s);
	
	return 0;
}
