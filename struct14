#include <stdio.h>
#include <string.h>

typedef struct student{																						//구조체 student 정의  
	char name[10];
	char name_f[3][10];
}person;																									//typedef 로 person으로 바궈줌 

int main(){ 
	person ar[3] = {"",{""}};																				//person 구조체 배열 선언 
	char name_to_find[10] = "";																				//입력 받을 이름 문자열 선언 
	int i = 0;
	
	for(i = 0;i<3;i++){
		scanf("%s -> %s%s%s",&ar[i].name,&ar[i].name_f[0],&ar[i].name_f[1],&ar[i].name_f[2]);				//3명의 학생이름과 각각 좋아하는 학생의 이름 3개 를 입력 받음 
	}
	
	scanf("%s",&name_to_find);																				 
	
	for(i=0;i<3;i++){
		if(strcmp(ar[i].name,name_to_find)==0){																//입력받은 이름과 구조체 배열의 name문자열이 같다면 
			printf(" %s %s %s\n",ar[i].name_f[0],ar[i].name_f[1],ar[i].name_f[2]);							//좋아하는 학생들의 이름을 출력 
		}
	}
	
	return 0;
}
