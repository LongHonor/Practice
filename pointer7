#include<stdio.h>

void swap(int *p,int *q);
int main(){
	int ar[50] = {0};
	int a, b,n,*p = ar;

	scanf("%d",&n);
	
	for(p = ar; p<ar+n;p++){
		scanf("%d",p);
	}

	scanf("%d %d",&a,&b);

	swap(ar+a,ar+b);

	for(p = ar; p<ar+n;p++){
		printf(" %d",*p);
	}
	printf("\n");

	return 0;
}

void swap(int *p, int *q){
	int tmp;

	tmp = *p;
	*p = *q;
	*q = tmp;
}
