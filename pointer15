#include <stdio.h>

void maxmin(int x[],int cnt);	//maxmin 함수 원형 선언 

int main(){
	int ar[100]={0};			//배열 선언 
	int *p = ar;				//배열 대신 사용할 포인터변수 선언 
	int i=0;
	
	while (1){					//정수를 입력받는 반복문 
		scanf("%d",(p+i));		
		if(*(p+i)==0) break;	//0이 입력되면 반복문 탈출 
		i++;
	}
	
	maxmin(ar,i);
	
	for(p=ar; p<(ar+2);p++){	//배열의 첫번째와 두번째 자리에 저장된 최댓값과 최솟값 출력 
		printf("%d ",*p);
	}
	
	return 0;
}

void maxmin(int x[],int cnt){
	int max = x[0], min = x[0];	//최댓값과 최솟값 변수 선언과 동시에 초기화 
	int *p = NULL;						//포인터 변수 선언과 동시에 초기화 
	
	for(p = x;p<(x+cnt);p++){			//주소를 통해 배열을 훑어보는 반복문 
		if(*p >= max) max = *p;			//최댓값 저장 
		if(*p <= min) min = *p;			//최솟값 저장 
	}
	
	*x = max;
	*(x+1) = min;
}
