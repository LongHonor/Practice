#include <stdio.h>

typedef struct student{
	int sex;
	int weight;
	int height;
}body; 

int main(){
	body ar[10] = {0,0,0};
	int n=0,i=0;
	int cnt1 = 0,cnt2 = 0,cnt3 = 0;
	
	scanf("%d",&n);
	
	for(i=0;i<n;i++){
		scanf("%d%d%d",&ar[i].sex,&ar[i].weight,&ar[i].height);
	}
	
	for(i=0;i<n;i++){
		if(ar[i].sex == 1){
			if(ar[i].height<165){
				if(ar[i].weight< 60) cnt1++;
				else if(ar[i].weight>=60&&ar[i].weight<70) cnt3++;
				else cnt2++;
			}
			else if(ar[i].height>=165&&ar[i].height<175){
				if(ar[i].weight< 60) cnt2++;
				else if(ar[i].weight>=60&&ar[i].weight<70) cnt1++;
				else cnt3++;
			}
			else{
				if(ar[i].weight< 60) cnt3++;
				else if(ar[i].weight>=60&&ar[i].weight<70) cnt2++;
				else cnt1++;
			}
		}
		else{
			if(ar[i].height<165){
				if(ar[i].weight<50) cnt1++;
				else if(ar[i].weight>=50&&ar[i].weight<60) cnt3++;
				else cnt2++;
			}
			else if(ar[i].height>=165&&ar[i].height<175){
				if(ar[i].weight<50) cnt2++;
				else if(ar[i].weight>=50&&ar[i].weight<60) cnt1++;
				else cnt3++;
			}
			else{
				if(ar[i].weight<50) cnt3++;
				else if(ar[i].weight>=50&&ar[i].weight<60) cnt2++;
				else cnt1++;
			}
		}
	}
	
	printf("%d %d %d\n",cnt1,cnt2,cnt3);
	
	return 0;
}
