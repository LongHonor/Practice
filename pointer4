#include<stdio.h>


int main(){
	int arr[5] = {0};
	int rank[5] = {0};
	int *p = NULL;
	int i = 0;

	for(p = arr; p<arr+5;p++){
		scanf("%d",p);
	}

	for(i=0;i<5;i++){
		for(p = arr;p<arr+5;p++){
			if(*(arr+i) < *p){
				*(rank+i)+=1;
			}
		}
	}
	p = arr;

	for(i = 0;i<5;i++){
		printf("%d=r%d ",*(p+i),*(rank+i)+1);
	}

	return 0;
}
